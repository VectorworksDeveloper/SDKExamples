ENABLE_OPENMP_SUPPORT_0430 = NO
ENABLE_OPENMP_SUPPORT_0440 = NO
ENABLE_OPENMP_SUPPORT_0450 = NO
ENABLE_OPENMP_SUPPORT_0460 = NO
ENABLE_OPENMP_SUPPORT_0500 = NO
ENABLE_OPENMP_SUPPORT = $(ENABLE_OPENMP_SUPPORT_$(XCODE_VERSION_MINOR))

