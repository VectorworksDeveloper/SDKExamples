MACOSX_DEPLOYMENT_TARGET_0420 = 10.6
MACOSX_DEPLOYMENT_TARGET_0410 = 10.6
MACOSX_DEPLOYMENT_TARGET_0400 = 10.5
MACOSX_DEPLOYMENT_TARGET_0320 = 10.5

REZ_PREPROCESSOR_DEFINITIONS = REZ_FOR_MACHO=1
SHARED_PRECOMPS_DIR = $(CONFIGURATION_TEMP_DIR)/PrecompiledHeader

DEBUG_INFORMATION_FORMAT = dwarf

VALID_ARCHS = i386 x86_64

GCC_VERSION_0310 = use_xcode_5_0_1
GCC_VERSION_0320 = use_xcode_5_0_1
GCC_VERSION_0410 = use_xcode_5_0_1
