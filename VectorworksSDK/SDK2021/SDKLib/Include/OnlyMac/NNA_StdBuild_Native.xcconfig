#include "NNA_BaseConfiguration.xcconfig"

ARCHS = $(ARCHS_STANDARD_32_64_BIT)
ONLY_ACTIVE_ARCH = YES


CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++

MACOSX_DEPLOYMENT_TARGET_0610 = 10.9
MACOSX_DEPLOYMENT_TARGET_0500 = 10.7
MACOSX_DEPLOYMENT_TARGET_0460 = 10.7
MACOSX_DEPLOYMENT_TARGET_0450 = 10.7
MACOSX_DEPLOYMENT_TARGET_0440 = 10.7
