GCC_VERSION	= $(GCC_VERSION_$(XCODE_VERSION_MINOR))
GCC_MODEL_TUNING = 

LD_OPENMP_FLAGS_0320 = -fopenmp
LD_OPENMP_FLAGS_0420 = 
LD_OPENMP_FLAGS_0430 =
LD_OPENMP_FLAGS_0440 =
LD_OPENMP_FLAGS_0450 = 
LD_OPENMP_FLAGS_0460 = 
LD_OPENMP_FLAGS_0500 =
LD_OPENMP_FLAGS = $(LD_OPENMP_FLAGS_$(XCODE_VERSION_MINOR))

ENABLE_OPENMP_SUPPORT_0320 = YES
